{
    "name": "duyler/builder",
    "type": "library",
    "description": "Duyler application builder",
    "license": "MIT",
    "keywords": [
        "framework"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/en/docs/builder/"
    },
    "require": {
        "php": "^8.4",
        "duyler/event-bus": "dev-main",
        "duyler/di": "dev-main",
        "duyler/config": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.77"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\Builder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Builder\\Test\\": ["tests/"]
        }
    }
}
